{
  "version": 2,
  "functions": {
    "index.js": {
      "includeFiles": "node_modules/pdfkit/js/data/**"
    }
  },
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
